{
  "name": "qalite-server",
  "version": "2.0.0",
  "description": "Proxy services",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsc -w -p tsconfig.json",
    "lint": "npm run build && eslint dist --ext .js",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "qa",
    "proxy",
    "slack"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.5.2",
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
